
rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    // WARNING: These rules are insecure and allow any authenticated user to read and write all data.
    // They are intended for temporary development and debugging only.
    // DO NOT use these rules in a production environment.
    match /{document=**} {
      allow read, write: if request.auth != null;
    }
  }
}
